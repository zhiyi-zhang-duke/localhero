{
	"name": "localhero",
	"displayName": "LocalHero",
	"description": "A vscode extension to document every stinkin local environment issue for your collaborative code base",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:localhero.logError",
		"onCommand:localhero.searchError",
		"onCommand:localhero.cats"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "localhero.logError",
				"category": "localhero",
				"title": "Log Error"
			},
			{
				"command": "localhero.cats",
				"category": "localhero",
				"title": "Cats"
			},
			{
				"command": "localhero.searchError",
				"category": "localhero",
				"title": "Search Existing Errors"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fuzzyset": "^1.0.4",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^14.18.3",
		"@types/vscode": "^1.63.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.5.4",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"fuzzyset": "^1.0.7",
		"fuzzysort": "^1.1.4"
	}
}
